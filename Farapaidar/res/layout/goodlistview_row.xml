<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/linearLayout1"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="#7099B2FF"
    android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/FifthText"
            android:layout_width="0dip"
            android:layout_height="30dp"
            android:layout_marginRight="10dp"
            android:layout_weight="0.02"
            android:gravity="right|center"
            android:text="Fifth"
            android:textDirection="rtl"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/SixthText"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginRight="10dp"
            android:layout_weight="0.00"
            android:gravity="right"
            android:text="Sixth"
            android:textAlignment="0"
            android:textDirection="rtl"
            android:textSize="16sp"
            android:textStyle="bold" >

        </TextView>

         <ImageView
             android:id="@+id/imageView1"
             android:layout_width="40dp"
             android:layout_height="40dp"
             android:src="@drawable/goodicon" />

    </LinearLayout>
    
    
        
     <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
         android:orientation="horizontal"  >
     
     

        <TextView
            android:id="@+id/SecondText"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:gravity="left|center"
            android:text="Second"
            android:textColor="#812"
            android:textDirection="rtl"
            android:layout_marginLeft="10dp"
            android:textSize="20sp"
            android:textStyle="bold" />

       
        
        
         <TextView
             android:id="@+id/PriceName"
             android:layout_width="wrap_content"
             android:layout_height="30dp"
             android:layout_marginRight="50dp"
             android:gravity="right"
             android:text="قیمت فروش : "
             android:textDirection="rtl"
             android:textSize="14sp" />

         
         
        
    </LinearLayout>
    
     
       <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
         android:orientation="horizontal"  >
     
     

        <TextView
            android:id="@+id/SecondTexttemp"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:gravity="left|center"
            android:text="-"
            android:textDirection="rtl"
            android:layout_marginLeft="10dp"
            android:textSize="16sp"
            android:textStyle="bold" />

       
        
        
         <TextView
             android:id="@+id/PriceNametemp"
             android:layout_width="wrap_content"
             android:layout_height="30dp"
             android:layout_marginRight="50dp"
             android:gravity="right"
             android:text="قیمت مصرف کننده : "
             android:textDirection="rtl"
             android:textSize="14sp" />

         
         
        
    </LinearLayout>
    
       <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
         android:orientation="horizontal"  >

    <TextView
        android:id="@+id/FirstText"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:gravity="left|center"
        android:text="First"
        android:textAlignment="0"
        android:textDirection="rtl"
        android:textSize="16sp"
        android:layout_marginLeft="10dp"
        android:textStyle="bold" />
    
     <TextView
         android:id="@+id/PriceTypeName"
         android:layout_width="wrap_content"
         android:layout_height="30dp"
         android:layout_marginRight="50dp"
         android:gravity="right"
         android:text="تعداد در کارتن : "
         android:textDirection="rtl"
         android:textSize="14sp" />

    </LinearLayout>
    
    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
         android:orientation="horizontal"  >

    
    
    <TextView
        android:id="@+id/FourthText"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:gravity="left|center"
        android:text="Fourth"
        android:textDirection="rtl"
        android:textSize="16sp"
        android:layout_marginLeft="10dp"
        android:textStyle="bold" >

    </TextView>

    <TextView
        android:id="@+id/CountUnitName"
        android:layout_width="100dp"
        android:layout_height="30dp"
         android:layout_marginRight="50dp"
        android:gravity="left|center"
        android:text="موجودی :"
        android:textDirection="rtl"
        android:textSize="14sp" />

   </LinearLayout>
   
    
    <LinearLayout 
        android:id="@+id/linearLayout6"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
         android:orientation="horizontal" >
    <TextView
        android:id="@+id/ThirdText"
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:layout_marginRight="30dp"
        android:gravity="left|center"
        android:text="Third"
        android:textDirection="rtl"
        android:textSize="16sp"
        android:layout_marginLeft="10dp"
        android:textStyle="bold" >

    </TextView>
        
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
     </LinearLayout>

</LinearLayout>